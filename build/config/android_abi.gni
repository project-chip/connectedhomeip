if (current_os == "android") {
  if (current_cpu == "arm") {
    android_abi = "armeabi-v7a"
  } else if (current_cpu == "arm64") {
    android_abi = "arm64-v8a"
  } else if (current_cpu == "x64") {
    android_abi = "x86_64"
  } else if (current_cpu == "x86") {
    android_abi = "x86"
  } else {
    assert(false, "Unsupported target_cpu: ${current_cpu}")
  }
}
