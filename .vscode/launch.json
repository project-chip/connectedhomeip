            "cwd": "${workspaceFolder}"
        },

        {
            "name": "OTA Requestor App (Linux)",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/out/ota-requestor/chip-ota-requestor-app",
            "args": [
                "--discriminator",
                "18",
                "--secured-device-port",
                "5560",
                "--KVS",
                "/tmp/chip_kvs_requestor"
