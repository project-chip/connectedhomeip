      <item value="0" name="Heat" summary="Adjust Heat Setpoint">
        <mandatoryConform>
          <feature name="HEAT"/>
        </mandatoryConform>
      </item>
      <item value="1" name="Cool" summary="Adjust Cool Setpoint">
        <mandatoryConform>
          <feature name="COOL"/>
        </mandatoryConform>
      </item>
      <item value="2" name="Both" summary="Adjust Heat Setpoint and Cool Setpoint">
