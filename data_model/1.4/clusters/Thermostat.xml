      <item value="6" name="Precooling" summary="(see Terms)">
        <optionalConform>
          <feature name="COOL"/>
        </optionalConform>
      </item>
      <item value="7" name="FanOnly">
        <optionalConform/>
      </item>
      <item value="8" name="Dry">
