<cluster xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="types types.xsd cluster cluster.xsd" id="0x0553" name="WebRTC Transport Provider Cluster" revision="1">
  <revisionHistory>
    <revision revision="1" summary="Initial version of the WebRTC Transport Provider Cluster"/>
  </revisionHistory>
  <clusterIds>
    <clusterId id="0x0553" name="WebRTC Transport Provider"/>
  </clusterIds>
  <classification hierarchy="base" role="application" picsCode="WEBRTCP" scope="Endpoint"/>
  <dataTypes>
    <number name="WebRTCSessionID" type="uint16"/>
    <enum name="WebRTCEndReasonEnum">
      <item value="0" name="IceFailed" summary="No media connection could be established to the other party">
        <mandatoryConform/>
      </item>
      <item value="1" name="IceTimeout" summary="The call timed out whilst waiting for ICE candidate gathering to complete">
        <mandatoryConform/>
      </item>
      <item value="2" name="UserHangup" summary="The user chose to end the call">
        <mandatoryConform/>
      </item>
      <item value="3" name="UserBusy" summary="The remote party is busy">
        <mandatoryConform/>
      </item>
      <item value="4" name="Replaced" summary="The call was replaced by another call">
        <mandatoryConform/>
      </item>
      <item value="5" name="NoUserMedia" summary="An error code when there is no local mic/camera to use. This may be because the hardware isn&apos;t plugged in, or the user has explicitly denied access">
        <mandatoryConform/>
      </item>
      <item value="6" name="InviteTimeout" summary="The call timed out whilst waiting for the offer/answer step to complete">
        <mandatoryConform/>
      </item>
      <item value="7" name="AnsweredElsewhere" summary="The call was answered from a different device">
        <mandatoryConform/>
      </item>
      <item value="8" name="OutOfResources" summary="The was unable to continue due to not enough resources or available streams">
        <mandatoryConform/>
      </item>
      <item value="9" name="MediaTimeout" summary="The call ended due to a media timeout">
        <mandatoryConform/>
      </item>
      <item value="10" name="LowPower" summary="The call ended due to hitting a low power condition">
        <mandatoryConform/>
      </item>
      <item value="11" name="UnknownReason" summary="Unknown or unspecified reason">
        <mandatoryConform/>
      </item>
    </enum>
    <bitmap name="WebRTCMetadataOptionsBitmap">
      <bitfield name="DataTLV" bit="0" summary="Reserved for future use">
        <optionalConform/>
      </bitfield>
    </bitmap>
    <struct name="ICEServerStruct">
      <field id="1" name="urls" type="list">
        <entry type="string"/>
        <mandatoryConform/>
      </field>
      <field id="2" name="username" type="string">
        <optionalConform/>
      </field>
      <field id="3" name="credential" type="string">
        <optionalConform/>
      </field>
      <field id="4" name="CAID" type="TLSCAID">
        <optionalConform/>
      </field>
    </struct>
    <struct name="WebRTCSessionStruct">
      <field id="1" name="ID" type="WebRTCSessionID">
        <mandatoryConform/>
      </field>
      <field id="2" name="PeerNodeID" type="node-id">
        <mandatoryConform/>
      </field>
      <field id="3" name="PeerFabricIndex" type="fabric-idx">
        <mandatoryConform/>
      </field>
      <field id="4" name="StreamUsage" type="StreamUsageEnum">
        <mandatoryConform/>
      </field>
      <field id="5" name="VideoStreamID" type="VideoStreamID">
        <quality nullable="true"/>
        <mandatoryConform/>
      </field>
      <field id="6" name="AudioStreamID" type="AudioStreamID">
        <quality nullable="true"/>
        <mandatoryConform/>
      </field>
      <field id="7" name="MetadataOptions" type="WebRTCMetadataOptionsBitmap" default="0">
        <mandatoryConform/>
      </field>
    </struct>
  </dataTypes>
  <attributes>
    <attribute id="0x0000" name="CurrentSessions" type="list">
      <entry type="WebRTCSessionStruct"/>
      <access read="true" readPrivilege="view"/>
      <mandatoryConform/>
    </attribute>
  </attributes>
  <commands>
    <command id="0x01" name="SolicitOffer" direction="commandToServer" response="SolicitOfferResponse">
      <quality largeMessage="true"/>
      <access invokePrivilege="operate"/>
      <mandatoryConform/>
      <field id="1" name="StreamUsage" type="StreamUsageEnum">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="2" name="VideoStreamID" type="VideoStreamID">
        <quality nullable="true"/>
        <optionalConform choice="a" more="true" min="1"/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="3" name="AudioStreamID" type="AudioStreamID">
        <quality nullable="true"/>
        <optionalConform choice="a" more="true" min="1"/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="4" name="ICEServers" type="list">
        <entry type="ICEServerStruct"/>
        <optionalConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="5" name="ICETransportPolicy" type="string">
        <optionalConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="6" name="MetadataOptions" type="WebRTCMetadataOptionsBitmap">
        <optionalConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
    </command>
    <command id="0x02" name="SolicitOfferResponse" direction="responseFromServer">
      <quality largeMessage="true"/>
      <mandatoryConform/>
      <field id="1" name="WebRTCSessionID" type="WebRTCSessionID">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="2" name="DeferredOffer" type="bool">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="3" name="VideoStreamID" type="VideoStreamID">
        <quality nullable="true"/>
        <mandatoryConform>
          <command name="SolicitOffer">
            <field name="VideoStreamID"/>
          </command>
        </mandatoryConform>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="4" name="AudioStreamID" type="AudioStreamID">
        <quality nullable="true"/>
        <mandatoryConform>
          <command name="SolicitOffer">
            <field name="AudioStreamID"/>
          </command>
        </mandatoryConform>
        <constraint>
          <desc/>
        </constraint>
      </field>
    </command>
    <command id="0x03" name="ProvideOffer" direction="commandToServer" response="ProvideOfferResponse">
      <quality largeMessage="true"/>
      <access invokePrivilege="operate"/>
      <mandatoryConform/>
      <field id="1" name="WebRTCSessionID" type="WebRTCSessionID" default="null">
        <quality nullable="true"/>
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="2" name="SDP" type="string">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="3" name="StreamUsage" type="StreamUsageEnum">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="4" name="VideoStreamID" type="VideoStreamID">
        <quality nullable="true"/>
        <optionalConform choice="c" more="true" min="1"/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="5" name="AudioStreamID" type="AudioStreamID">
        <quality nullable="true"/>
        <optionalConform choice="c" more="true" min="1"/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="6" name="ICEServers" type="list">
        <entry type="ICEServerStruct"/>
        <optionalConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="7" name="ICETransportPolicy" type="string">
        <optionalConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="8" name="MetadataOptions" type="WebRTCMetadataOptionsBitmap">
        <optionalConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
    </command>
    <command id="0x04" name="ProvideOfferResponse" direction="responseFromServer">
      <quality largeMessage="true"/>
      <mandatoryConform/>
      <field id="1" name="WebRTCSessionID" type="WebRTCSessionID">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="2" name="VideoStreamID" type="VideoStreamID">
        <quality nullable="true"/>
        <mandatoryConform>
          <command name="ProvideOffer">
            <field name="VideoStreamID"/>
          </command>
        </mandatoryConform>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="3" name="AudioStreamID" type="AudioStreamID">
        <quality nullable="true"/>
        <mandatoryConform>
          <command name="ProvideOffer">
            <field name="AudioStreamID"/>
          </command>
        </mandatoryConform>
        <constraint>
          <desc/>
        </constraint>
      </field>
    </command>
    <command id="0x05" name="ProvideAnswer" direction="commandToServer" response="Y">
      <quality largeMessage="true"/>
      <access invokePrivilege="operate"/>
      <mandatoryConform/>
      <field id="1" name="WebRTCSessionID" type="WebRTCSessionID">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="2" name="SDP" type="string">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
    </command>
    <command id="0x06" name="ProvideICECandidates" direction="commandToServer" response="Y">
      <quality largeMessage="true"/>
      <access invokePrivilege="operate"/>
      <mandatoryConform/>
      <field id="1" name="WebRTCSessionID" type="WebRTCSessionID">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="2" name="ICECandidates" type="list">
        <entry type="string"/>
        <mandatoryConform/>
        <constraint>
          <minCount value="1"/>
        </constraint>
      </field>
    </command>
    <command id="0x07" name="EndSession" direction="commandToServer" response="Y">
      <quality largeMessage="true"/>
      <access invokePrivilege="operate"/>
      <mandatoryConform/>
      <field id="1" name="WebRTCSessionID" type="WebRTCSessionID">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
      <field id="2" name="Reason" type="WebRTCEndReasonEnum" default="UnknownReason">
        <mandatoryConform/>
        <constraint>
          <desc/>
        </constraint>
      </field>
    </command>
  </commands>
</cluster>
