      <field id="0" name="AudioStreamID" type="AudioStreamID">
        <mandatoryConform/>
      </field>
      <field id="1" name="StreamUsage" type="StreamUsageEnum">
        <mandatoryConform/>
      </field>
      <field id="2" name="AudioCodec" type="AudioCodecEnum">
        <mandatoryConform/>
      </field>
      <field id="3" name="ChannelCount" type="uint8">
        <mandatoryConform/>
