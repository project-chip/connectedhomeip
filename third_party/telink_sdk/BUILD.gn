import("//build_overrides/telink_sdk.gni")
import("${telink_sdk_build_root}/telink_sdk.gni")

declare_args() {
  # Build target to use for QPG SDK. Use this to set global SDK defines.
  telink_sdk_target = ""
}

assert(telink_sdk_target != "", "telink_sdk_target must be specified")

group("telink_sdk") {
  public_deps = [ telink_sdk_target ]
}