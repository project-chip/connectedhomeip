      "${bl_iot_sdk_root}/components/platform/soc/bl702/bl702_std/BSP_Driver/std_drv/src/bl702_xip_sflash_ext.c",
    ]

    configs += [
      ":${sdk_target_name}_config",
      ":${sdk_target_name}_config_freertos",
