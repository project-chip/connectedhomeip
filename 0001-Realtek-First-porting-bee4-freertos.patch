+uint16_t BLEManagerImpl::_NumConnections()
+{
+    uint16_t numCons = 0;
+    for (uint16_t i = 0; i < kMaxConnections; i++)
+    {
+        if (mSubscribedConIds[i] != BLE_CONNECTION_UNINITIALIZED)
+        {
+            numCons++;
+        }
+    }
+
+    return numCons;
