<!--
  Test cluster for MEI handling validation.
  
  This cluster is intentionally NOT included in controller-clusters.matter
  to simulate a real-world scenario where a device has a vendor-specific 
  MEI cluster that the controller/test harness has no knowledge of.
  
  Used to validate TC_IDM_2_2 and other tests correctly handle unknown MEI clusters.
  
  Cluster ID: 0xFFF1FC21 (4294048801)
  - Prefix 0xFFF1: Test vendor ID range
  - Suffix 0xFC21: MEI cluster range
-->
<configurator>
  <domain name="CHIP"/>

  <cluster apiMaturity="internal">
    <domain>CHIP</domain>
    <name>Test Hidden Manufacturer Specific</name>
    <code>0xFFF1FC21</code>
    <define>TEST_HIDDEN_MEI_CLUSTER</define>
    <description>Test cluster for validating MEI handling without controller-side implementation</description>

    <attribute side="server" code="0x0000" define="TEST_ATTRIBUTE" type="boolean" writable="false" default="true" optional="false">TestAttribute</attribute>
  </cluster>
</configurator>
